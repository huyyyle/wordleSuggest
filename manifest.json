{
    "name": "WordleSuggest",
    "description": "Word autocomplete for Wordle.",
    "version": "1.0.2",
    "manifest_version": 3,
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": ["https://www.nytimes.com/games/wordle/index.html"],
            "js": ["wordlesuggest.js", "./src/controller.js","./src/model.js","./src/view.js" ]
        }
    ]
}