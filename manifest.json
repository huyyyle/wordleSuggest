{
    "name": "WordleSuggest",
    "description": "Quick word suggestion for Worlde",
    "version": "0.0.1",
    "manifest_version": 3,

    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": "https://www.nytimes.com/games/wordle/index.html",
            "css": ["wordlesuggest.css"],
            "js": ["wordlesuggest.js"]
        }
    ]
}